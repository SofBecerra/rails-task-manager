<h1>My Task</h1>
<div>
    <% @tasks.each do |task| %>
      <div>
        <% if task.completed %>
        <%= "[x]" %>
        <% else %>
        <%= "[ ]" %>
        <% end %>
        <%= link_to task.title, task_path(task) %>
      </div>
    <% end %>
</div>

<h2><%= link_to "Add new", new_task_path %> </h2>
